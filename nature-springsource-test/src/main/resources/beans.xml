<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="person" class="com.nature.spring.bean.Person">
		<property name="name" value="nature"></property>
	</bean>

	<bean id="cat" class="com.nature.spring.bean.Cat">
		<property name="name" value="cat"></property>
	</bean>

	<!--工厂bean-->
	<bean id="hello" class="com.nature.spring.bean.HelloFactoryBean">

	</bean>
</beans>